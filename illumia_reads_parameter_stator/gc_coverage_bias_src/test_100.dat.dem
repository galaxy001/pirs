reset
set xrange [-1:101]
#set yrange [0:10]
#set y2range [0:100]
set border 11
set xtics 0,5 nomirror
set ytics nomirror
set y2tics nomirror

set term png font "/ifs1/ST_ASMB/USER/huxuesong/public/fonts/arial.ttf" 16 size 1600,1200
set output "test_100.dat.png"
set title "GC-dep plot of window_size 100"
set xlabel "GC %"
set ylabel "Depth"
set y2label "Ref. Windows Count"
set boxwidth 0.8
set style fill empty
plot 'test_100.dat' using 1:6:5:9:8 with candlesticks lt 3 lw 2 title 'depth' whiskerbars 0.5, \
     ''      using 1:7:7:7:7 with candlesticks lt -1 lw 2 notitle, \
     ''      using 1:4 lt rgb "red" lw 2 title 'mean depth', \
     ''      using 1:3 with points axis x1y2 lt rgb "brown" lw 1 notitle, \
     ''      using 1:11 with points lt rgb "royalblue" lw 1 title 'max depth', \
     ''      using 1:3 smooth csplines axis x1y2 lt rgb "brown" lw 1 title 'Ref. win. cnt.'
#pause -1 "Hit return to continue"
